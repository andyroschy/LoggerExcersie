<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=(LocalDb)\v11.0;Integrated Security=True;Initial Catalog=assignamentdb" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="LogDirectory"/>
    <add key="LogLevel" value="Info"/>
    <add key="LogDirectory" value="C:\Logs"/>
    <!--List of comma separeted values indicating which appenders should be active. Possible values = Console, File, Database-->
    <add key="ActiveAppenders" value="Console,File,Database"/>
  </appSettings>
</configuration>